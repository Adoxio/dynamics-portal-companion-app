<hr/><h4>Dynamics Portal Companion App API SDK Test</h4><p><button id="run-crmsdk" class="btn btn-primary" onclick="sdkTest();">Run Test</button></p><pre id="crmsdk" style="max-height:300px;overflow-y:scroll;white-space: pre-wrap;"></pre><hr/>{% raw %}<script>	var sdkTest = function () {	    $('#crmsdk').html('Running Execute Request...');			DynPCA.App.ExecuteRequest('GET', null, 'json', true, '/api/crmsdk', null).done(				function (data){				  console.log(data);					$('#crmsdk').html(JSON.stringify(data, null, 2));				}).fail( 				function (jqXHR, textStatus, errorThrown) {					$('#crmsdk').html(errorThrown);				});		};</script>{% endraw %}